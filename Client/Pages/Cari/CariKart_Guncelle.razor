@page "/cari_kartf_guncelle/{CariKartRef:int}"

<HeaderSearchBar header="header" ShowSearchSection="false"></HeaderSearchBar>

<CariKartEkleGuncelleComponent CariKartRef="CariKartRef"></CariKartEkleGuncelleComponent>

@code
{
    [Parameter]
    public int? CariKartRef { get; set; }

    [Inject]
    NavigationManager navigation { get; set; }

    public HeaderBarSearch header = new HeaderBarSearch();

    protected override Task OnInitializedAsync()
    {
        header.HeaderText = "Cari Kart Güncelle";
        return Task.CompletedTask;
    }

    public void goBack()
    {
        navigation.NavigateTo("/cari_kartf");
    }


}
